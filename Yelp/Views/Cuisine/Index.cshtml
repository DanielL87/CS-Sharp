@{
    Layout = "_Layout";
}
<h2>Cuisines</h2>

<ul>
@if(@Model.Count == 0)
{
    <h4>You have no cuisines!</h4>
    <h4>Click <a href="/cuisine/new"> here </a> to add some now.</h4>
    <h4>Click <a href="/">HERE</a> to return to main page.</h4>
}
else
{
@foreach(var cuisine in Model)
{
  <li><a href="/cuisine/show/@cuisine.GetId()">@cuisine.GetName()</a></li>
}

<h4>Click <a href="/restaurant/new">HERE</a> to add new restaurant.</h4>

}

</ul>

